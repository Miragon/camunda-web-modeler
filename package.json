{
    "name": "@miragon/camunda-web-modeler-test",
    "version": "0.0.5",
    "description": "A browser-native BPMN and DMN modeler based on bpmn.io.",
    "author": "FlowSquad GmbH <info@flowsquad.io>",
    "homepage": "https://github.com/FlowSquad/camunda-web-modeler",
    "scripts": {
        "clean": "rimraf dist index.js index.d.ts",
        "build": "npx tsc && npx tsc -p tsconfig.index.json && copyfiles -u 1 \"src/types/*.d.ts\" dist",
        "bundle": "npx rollup -c",
        "lint": "npx eslint src --ext .js,.jsx,.ts,.tsx"
    },
    "license": "Apache-2.0",
    "files": [
        "dist/",
        "index.d.ts",
        "index.js"
    ],
    "main": "index.js",
    "directories": {
        "lib": "dist/"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@material-ui/styles": "^4.11.4",
        "@uiw/react-monacoeditor": "^3.2.2",
        "bpmn-js": "^8.7.0",
        "bpmn-js-properties-panel": "^0.43.1",
        "camunda-bpmn-moddle": "^5.1.2",
        "camunda-dmn-moddle": "^1.1.0",
        "clsx": "^1.1.1",
        "deepmerge": "^4.2.2",
        "diagram-js-origin": "^1.3.2",
        "dmn-js": "^10.3.0",
        "dmn-js-properties-panel": "^0.6.1",
        "react-split-pane": "^0.1.92"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@rollup/plugin-replace": "^2.4.2",
        "@types/react": "^17.0.11",
        "@typescript-eslint/eslint-plugin": "^4.27.0",
        "@typescript-eslint/parser": "^4.27.0",
        "babel-eslint": "^10.1.0",
        "copyfiles": "^2.4.1",
        "eslint": "^7.28.0",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-config-react-app": "^6.0.0",
        "eslint-plugin-flowtype": "^5.7.2",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-app": "^6.2.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "react": "^17.0.0",
        "rollup": "^2.52.2",
        "rollup-plugin-css-only": "^3.1.0",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "tslib": "^2.3.0",
        "typescript": "^4.3.4"
    },
    "peerDependencies": {
        "react": "^17.0.0"
    }
